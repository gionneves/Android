<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DDCCCCCC"
    android:saveEnabled="true"
    tools:context=".TelaCadastro">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="1167dp">

                <TextView
                    android:id="@+id/textViewTelefoneAlt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="Telefone alternativo:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetTelefone2"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetTelefone2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:hint="@string/text_telefoneAlt"
                    android:inputType="phone"
                    app:layout_constraintBottom_toTopOf="@+id/textGetEndereco"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="*E-mail:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetEmail"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetCPF"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintBottom_toTopOf="@+id/textGetPhone"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetEndereco"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    app:layout_constraintBottom_toTopOf="@+id/textGetNumeroEnde"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetPhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:hint="@string/text_telefone"
                    android:inputType="phone"
                    app:layout_constraintBottom_toTopOf="@+id/textGetTelefone2"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetPass"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:hint="@string/get_text_password"
                    android:inputType="textPassword"
                    app:layout_constraintBottom_toTopOf="@+id/textGetAniver"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetAniver"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:hint="@string/data_placeholder"
                    android:inputType="date"
                    app:layout_constraintBottom_toTopOf="@+id/textGetCPF"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewCPF"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="*CPF:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetCPF"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewSenha"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="*Senha:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetPass"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewDataNasc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="*Data de nascimento:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetAniver"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:hint="@string/get_text_email"
                    android:inputType="textEmailAddress"
                    app:layout_constraintBottom_toTopOf="@+id/textGetPass"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewTelefone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="*Telefone"
                    app:layout_constraintBottom_toTopOf="@+id/textGetPhone"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewEndereco"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="Endereço:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetEndereco"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetNumeroEnde"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:inputType="number"
                    app:layout_constraintBottom_toTopOf="@+id/textGetCEP"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewNumeroEnde"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="Número:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetNumeroEnde"
                    app:layout_constraintStart_toStartOf="parent" />

                <EditText
                    android:id="@+id/textGetCEP"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text=""
                    app:layout_constraintBottom_toTopOf="@+id/textGetMunicipio"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewCEP"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="CEP:"
                    app:layout_constraintBottom_toTopOf="@+id/textGetCEP"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewCadastrese"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:gravity="start|right"
                    android:text="Cadastre-se"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <EditText
                    android:id="@+id/textGetMunicipio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="64dp"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Name"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textViewMunicipio"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:text="TextView"
                    app:layout_constraintBottom_toTopOf="@+id/textGetMunicipio"
                    app:layout_constraintStart_toStartOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>